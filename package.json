{
	"name": "fims",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"es:fix": "eslint --cache --fix .",
		"fix": "pnpm fmt:fix && pnpm es:fix",
    "fmt": "prettier --experimental-cli --check .",
		"fmt:fix": "prettier --experimental-cli --write .",
		"lint": "pnpm --parallel /^lint:/",
		"lint:es": "eslint --cache .",
		"lint:sv": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"test": "pnpm test:unit -- --run && pnpm test:e2e",
		"test:e2e": "playwright test",
		"test:unit": "vitest"
	},
	"devDependencies": {
		"@bastidood/eslint-plugin-imsort": "^0.11.0",
		"@eslint/css": "0.9.0",
		"@eslint/js": "^9.39.2",
		"@html-eslint/eslint-plugin": "^0.54.0",
		"@playwright/test": "^1.58.0",
		"@sveltejs/adapter-node": "^5.5.2",
		"@sveltejs/kit": "^2.50.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@tailwindcss/vite": "^4.1.18",
		"@types/node": "^22",
		"@vitest/browser-playwright": "^4.0.18",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.14.0",
		"globals": "^17.1.0",
		"playwright": "^1.58.0",
		"prettier": "^3.8.1",
		"prettier-plugin-svelte": "^3.4.1",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"svelte-check": "^4.3.5",
		"tailwindcss": "^4.1.18",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.53.1",
		"vite": "^7.3.1",
		"vitest": "^4.0.18",
		"vitest-browser-svelte": "^2.0.2"
	},
	"dependencies": {
		"@iconify/svelte": "^5.2.1",
		"pg": "^8.17.2",
		"svelte": "5.49.1",
		"valibot": "^1.2.0"
	}
}
